{
    "keymaps": {
        "0": { "type": "scroll.home" },
        ":": { "type": "command.show" },
        "o": { "type": "command.show.open", "alter": false },
        "O": { "type": "command.show.open", "alter": true },
        "t": { "type": "command.show.tabopen", "alter": false },
        "T": { "type": "command.show.tabopen", "alter": true },
        "w": { "type": "command.show.winopen", "alter": false },
        "W": { "type": "command.show.winopen", "alter": true },
        "b": { "type": "command.show.buffer" },
        "a": { "type": "command.show.addbookmark", "alter": true },
        "k": { "type": "scroll.vertically", "count": -1 },
        "j": { "type": "scroll.vertically", "count": 1 },
        "~h": { "type": "scroll.horizonally", "count": -1 },
        "~l": { "type": "scroll.horizonally", "count": 1 },
        "<C-U>": { "type": "scroll.pages", "count": -0.5 },
        "<C-D>": { "type": "scroll.pages", "count": 0.5 },
        "~<C-B>": { "type": "scroll.pages", "count": -1 },
        "~<C-F>": { "type": "scroll.pages", "count": 1 },
        "gg": { "type": "scroll.top" },
        "G": { "type": "scroll.bottom" },
        "$": { "type": "scroll.end" },
        "d": { "type": "tabs.close" },
        "D": { "type": "tabs.close", "select": "left" },
        "x$": { "type": "tabs.close.right" },
        "!d": { "type": "tabs.close", "force": true },
        "u": { "type": "tabs.reopen" },
        "h": { "type": "tabs.prev" },
        "l": { "type": "tabs.next" },
        "K": { "type": "tabs.prev" },
        "J": { "type": "tabs.next" },
        "gT": { "type": "tabs.prev" },
        "gt": { "type": "tabs.next" },
        "g0": { "type": "tabs.first" },
        "g$": { "type": "tabs.last" },
        "<C-6>": { "type": "tabs.prevsel" },
        "r": { "type": "tabs.reload", "cache": false },
        "R": { "type": "tabs.reload", "cache": true },
        "zp": { "type": "tabs.pin.toggle" },
        "zd": { "type": "tabs.duplicate" },
        "zi": { "type": "zoom.in" },
        "zo": { "type": "zoom.out" },
        "zz": { "type": "zoom.neutral" },
        "f": { "type": "follow.start", "newTab": false },
        "F": { "type": "follow.start", "newTab": true, "background": true },
        "m": { "type": "mark.set.prefix" },
        "'": { "type": "mark.jump.prefix" },
        "H": { "type": "navigate.history.prev" },
        "L": { "type": "navigate.history.next" },
        "[[": { "type": "navigate.link.prev" },
        "]]": { "type": "navigate.link.next" },
        "gu": { "type": "navigate.parent" },
        "gU": { "type": "navigate.root" },
        "gi": { "type": "focus.input" },
        "gf": { "type": "navigate.source" },
        "gh": { "type": "navigate.home" },
        "gH": { "type": "navigate.home", "newTab": true },
        "gr": { "type": "tabs.reader.toggle" },
        "y": { "type": "urls.yank" },
        "p": { "type": "urls.paste", "newTab": false },
        "P": { "type": "urls.paste", "newTab": true },
        "/": { "type": "find.start" },
        "n": { "type": "find.next" },
        "N": { "type": "find.prev" },
        ".": { "type": "repeat" },
        "<S-Esc>": { "type": "addon.toggle.enabled" }
    },
    "search": {
        "default": "google",
        "engines": {
            "google": "https://google.com/search?q={}",
            "yahoo": "https://search.yahoo.com/search?p={}",
            "bing": "https://www.bing.com/search?q={}",
            "dd": "https://duckduckgo.com/?q={}",
            "wikipedia": "https://en.wikipedia.org/w/index.php?search={}",
            "twitter": "https://twitter.com/search?q={}",
            "az": "https://www.amazon.com/s/?field-keywords={}",
            "drive": "https://drive.google.com/drive/u/0/search?q={}",
            "imdb": "http://www.imdb.com/find?q={}",
            "ciu": "https://caniuse.com/#search={}",
            "tpb": "https://thepiratebay.rocks/search/{}",
            "red": "https://www.reddit.com/search?q={}",
            "gf": "https://www.google.com/search?&tbm=fin&q={}",
            "ll": "https://lucidlife.wordpress.com/?s={}",
            "sb": "https://stackblitz.com/fork/{}",
            "tv": "https://www.tradingview.com/chart/?symbol={}",
            "yt": "https://www.youtube.com/results?search_query={}",
            "bl": "https://www.browserling.com/browse/win/7/chrome/72/{}",
            "dv": "https://www.dividend.com/search?q={}",
            "fv": "https://finviz.com/quote.ashx?t={}",
            "gd": "https://drive.google.com/drive/search?q={}",
            "sd": "https://slickdeals.net/newsearch.php?rating=2&q={}",
            "st": "https://stocktwits.com/symbol/{}",
            "yf": "https://finance.yahoo.com/quote/{}/financials"
        }
    },
    "properties": {
        "hintchars": "abcdefghijklmnopqrstuvwxyz",
        "smoothscroll": false,
        "complete": "sbh",
        "colorscheme": "system"
    },
    "blacklist": [
        { "url": "mail.google.com", "keys": [".", "/", "a", "f", "g", "j", "k", "o", "s", "u", "x", "J", "K", "U"] },
        { "url": "www.youtube.com/watch", "keys": ["f", "j", "k", "<", ">", ",", "."] },
        { "url": "twitch.tv/videos ", "keys": ["f", ",", "."] }
    ]
}

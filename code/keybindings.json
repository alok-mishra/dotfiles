////// Keybindings //////
[
    // Editor //
    {
        "key": "ctrl+,",
        "command": "workbench.action.openSettingsJson"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "ctrl+h",
        "command": "-editor.action.startFindReplaceAction",
        "when": "editorFocus || editorIsOpen"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "alt+d",
        "command": "workbench.action.closeActiveEditor",
        "when": "editorFocus"
    },
    {
        "key": "alt+shift+d",
        "command": "workbench.action.closeAllEditors"
    },
    {
        "key": "ctrl+p",
        "command": "focusPreviousCodeAction",
        "when": "CodeActionMenuVisible"
    },
    {
        "key": "ctrl+p",
        "command": "workbench.action.quickOpenSelectPrevious",
        "when": "inQuickOpen"
    },
    {
        "key": "ctrl+n",
        "command": "workbench.action.quickOpenSelectNext",
        "when": "inQuickOpen"
    },
    {
        "key": "ctrl+n",
        "command": "focusNextCodeAction",
        "when": "CodeActionMenuVisible"
    },
    {
        "key": "ctrl+n",
        "command": "-workbench.action.files.newUntitledFile"
    },
    {
        "key": "shift+alt+,",
        "command": "workbench.action.moveEditorLeftInGroup"
    },
    {
        "key": "shift+alt+.",
        "command": "workbench.action.moveEditorRightInGroup"
    },
    {
        "key": "ctrl+alt+r",
        "command": "workbench.action.reloadWindow"
    },
    {
        "key": "ctrl+r",
        "command": "-workbench.action.openRecent"
    },
    {
        "key": "ctrl+alt+p",
        "command": "workbench.action.openRecent"
    },
    {
        "key": "ctrl+alt+,",
        "command": "workbench.action.openGlobalKeybindingsFile"
    },
    {
        "key": "alt+s",
        "command": "workbench.action.files.save"
    },
    {
        "key": "alt+shift+s",
        "command": "workbench.action.files.saveWithoutFormatting"
    },
    {
        "key": "ctrl+alt+l",
        "command": "workbench.action.moveEditorToRightGroup"
    },
    {
        "key": "ctrl+alt+h",
        "command": "workbench.action.moveEditorToLeftGroup"
    },
    {
        "key": "ctrl+alt+k",
        "command": "editor.emmet.action.incrementNumberByOne"
    },
    {
        "key": "ctrl+shift+alt+k",
        "command": "editor.emmet.action.incrementNumberByTen"
    },
    {
        "key": "ctrl+alt+j",
        "command": "editor.emmet.action.decrementNumberByOne"
    },
    {
        "key": "ctrl+shift+alt+j",
        "command": "editor.emmet.action.decrementNumberByTen"
    },
    {
        "key": "ctrl+f",
        "command": "editor.action.startFindReplaceAction",
        "when": "editorFocus || editorIsOpen"
    },
    {
        "key": "ctrl+alt+=",
        "command": "editor.action.fontZoomIn"
    },
    {
        "key": "ctrl+alt+-",
        "command": "editor.action.fontZoomOut"
    },
    {
        "key": "ctrl+alt+0",
        "command": "editor.action.fontZoomReset"
    },
    {
        "key": "ctrl+alt+b",
        "command": "simpleBrowser.show"
    },
    {
        "key": "ctrl+alt+t",
        "command": "editor.action.peekTypeDefinition"
    },
    {
        "key": "alt+n",
        "command": "workbench.action.files.newUntitledFile"
    },
    {
        "key": "shift+o",
        "command": "workbench.files.action.expandExplorerFolders",
        "when": "filesExplorerFocus && !inputFocus"
    },
    {
        "key": "ctrl+alt+f",
        "command": "editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
    },
    {
        "key": "alt+/",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+/",
        "command": "-editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },

    // Explorer //
    {
        "key": "insert",
        "command": "explorer.newFile",
        "when": "filesExplorerFocus"
    },
    {
        "key": "shift+insert",
        "command": "explorer.newFolder",
        "when": "filesExplorerFocus"
    },
    {
        "key": "c",
        "command": "workbench.files.action.collapseExplorerFolders",
        "when": "filesExplorerFocus && !inputFocus"
    },
    {
        "key": "c",
        "command": "search.action.collapseSearchResults",
        "when": "searchViewletFocus && !inputFocus"
    },
    {
        "key": "f",
        "command": "filesExplorer.findInFolder",
        "when": "explorerResourceIsFolder && explorerViewletVisible && filesExplorerFocus && !inputFocus"
    },
    {
        "key": "e",
        "command": "revealFileInOS",
        "when": "filesExplorerFocus && !inputFocus && !editorFocus && remoteName != 'wsl'"
    },
    {
        "key": "e",
        "command": "remote-wsl.revealInExplorer",
        "when": "filesExplorerFocus && !inputFocus && !editorFocus && remoteName == 'wsl'"
    },

    // Panes //
    {
        "key": "alt+0",
        "command": "workbench.action.evenEditorWidths"
    },
    {
        "key": "alt+=",
        "command": "workbench.action.increaseViewWidth"
    },
    {
        "key": "alt+-",
        "command": "workbench.action.decreaseViewWidth"
    },
    {
        "key": "alt+h",
        "command": "workbench.action.navigateLeft"
    },
    {
        "key": "alt+l",
        "command": "workbench.action.navigateRight"
    },
    {
        "key": "alt+`",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "!editorTextFocus"
    },
    {
        "key": "alt+`",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+`",
        "command": "workbench.action.toggleEditorWidths"
    },
    {
        "key": "shift+alt+`",
        "command": "workbench.action.toggleMaximizedPanel",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+alt+j",
        "command": "workbench.action.moveEditorToBelowGroup"
    },
    {
        "key": "ctrl+alt+k",
        "command": "workbench.action.moveEditorToAboveGroup"
    },
    {
        "key": "alt+k",
        "command": "workbench.action.navigateUp"
    },
    {
        "key": "alt+j",
        "command": "workbench.action.navigateDown"
    },

    // Terminal //
    {
        "key": "t",
        "command": "openInTerminal",
        "when": "filesExplorerFocus && !inputFocus && !editorFocus"
    },
    {
        "key": "ctrl+`",
        "command": "workbench.action.terminal.focus",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+shift+`",
        "command": "workbench.action.terminal.split",
        "when": "terminalFocus && terminalProcessSupported || terminalFocus && terminalWebExtensionContributedProfile"
    },
    {
        "key": "alt+h",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus"
    },
    {
        "key": "alt+l",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus"
    },
    {
        "key": "alt+=",
        "command": "workbench.action.terminal.resizePaneUp",
        "when": "terminalFocus"
    },
    {
        "key": "alt+-",
        "command": "workbench.action.terminal.resizePaneDown",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+shift+alt+`",
        "command": "workbench.action.terminal.openNativeConsole",
        "when": "!terminalFocus"
    },
    {
        "key": "ctrl+shift+c",
        "command": "-workbench.action.terminal.openNativeConsole",
        "when": "!terminalFocus"
    },

    // Extensions //
    {
        "key": "ctrl+alt+v",
        "command": "markdown.showPreviewToSide",
        "when": "editorLangId == 'markdown'"
    },
    {
        "key": "ctrl+shift+alt+s",
        "command": "workbench.userDataSync.actions.syncNow"
    },
    {
        "key": "shift+enter",
        "command": "python.execInTerminal",
        "when": "editorFocus && !findInputFocussed && !replaceInputFocussed && !editorHasSelection && editorLangId == 'python'"
    },
    {
        "key": "ctrl+alt+c",
        "command": "workbench.action.chat.openInSidebar"
    },
    {
        "key": "ctrl+alt+w",
        "command": "remote-wsl.connect"
    },
    {
        "key": "ctrl+shift+alt+c",
        "command": "github.copilot.toggleCopilot"
    },
    {
        "key": "alt+m",
        "command": "vscode-editor-group-minimizer.minimize"
    },
    {
        "key": "shift+alt+m",
        "command": "vscode-editor-group-minimizer.addToGroup"
    },
    {
        "key": "ctrl+alt+m",
        "command": "vscode-editor-group-minimizer.restore"
    },
    {
        "key": "ctrl+alt+v",
        "command": "svgPreview.showPreviewToSide",
        "when": "resourceExtname == '.svg'"
    },
    {
        "key": "ctrl+k v",
        "command": "-svgPreview.showPreviewToSide",
        "when": "resourceExtname == '.svg'"
    },
    {
        "key": "shift+alt+/",
        "command": "github.copilot.interactiveEditor.explain.palette"
    },
    {
        "key": "shift+alt+=",
        "command": "workbench.action.increaseViewHeight"
    },
    {
        "key": "shift+alt+-",
        "command": "workbench.action.decreaseViewHeight"
    },
    {
        "key": "shift+alt+w",
        "command": "editor.emmet.action.wrapWithAbbreviation"
    },
    {
        "key": "alt+g",
        "command": "git-graph.view"
    },
    {
        "key": "ctrl+shift+alt+,",
        "command": "workbench.action.openGlobalKeybindings"
    },
    {
        "key": "ctrl+k ctrl+s",
        "command": "-workbench.action.openGlobalKeybindings"
    },
    {
        "key": "ctrl+shift+w",
        "command": "-workbench.action.closeWindow"
    },
    {
        "key": "alt+p",
        "command": "workbench.profiles.actions.switchProfile"
    },
    {
        "key": "ctrl+alt+\\",
        "command": "workbench.panel.aichat"
    },
    {
        "key": "alt+\\",
        "command": "composer.startComposerPrompt"
    },
    {
        "key": "ctrl+i",
        "command": "-composer.startComposerPrompt"
    },
    {
        "key": "ctrl+0",
        "command": "workbench.action.zoomReset"
    },
    {
        "key": "ctrl+numpad0",
        "command": "-workbench.action.zoomReset"
    },
    {
        "key": "shift+alt+a",
        "command": "composerMode.agent"
    },
    {
        "key": "alt+a",
        "command": "composerMode.chat"
    },
    {
        "key": "alt+z",
        "command": "workbench.action.toggleZenMode",
        "when": "!isAuxiliaryWindowFocusedContext"
    },
    {
        "key": "ctrl+m z",
        "command": "-workbench.action.toggleZenMode",
        "when": "!isAuxiliaryWindowFocusedContext"
    },
    {
        "key": "alt+w",
        "command": "editor.action.toggleWordWrap"
    },
    {
        "key": "alt+z",
        "command": "-editor.action.toggleWordWrap"
    }
]
